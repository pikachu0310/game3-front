    "gen:api-mock": "msw-auto-mock https://raw.githubusercontent.com/traPtitech/game3-back/main/docs/openapi.yaml -o ./mocks/handlers.server.ts --node --base-url http://localhost:3000/api && msw-auto-mock https://raw.githubusercontent.com/traPtitech/game3-back/main/docs/openapi.yaml -o ./mocks/handlers.client.ts --base-url http://localhost:3000/api"
  },
